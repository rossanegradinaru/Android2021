<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ro.ase.eatandtrack">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EatAndTrack">
        <activity android:name=".ActivitateAdaugareMasuratori"></activity>
        <activity android:name=".ActivitateHome" />
        <activity android:name=".ActivitateSpecialisti" />
        <activity android:name=".ActivitateMeniu" />
        <activity android:name=".ActivitatePrimireDate" />
        <activity android:name=".ActivitateAutentificare" />
        <activity android:name=".ActivitateCreareCont" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>